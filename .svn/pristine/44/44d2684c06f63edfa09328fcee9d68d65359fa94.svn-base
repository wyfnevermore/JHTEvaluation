package com.chinaias.entity;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

/** 原料 */
@Entity
@Table(name = "EV_MATERIAL", schema = "jhteval")
public class Material {
   /** id*/
   public String id;
   /** 材料清单 */
   public String listID;
   /** 物料序号 */
   public String materialNo;
   /** 物料名称*/
   public String materialName;
   /** 物料描述 */
   public String materialDesc;
   /** 加工方式 */
   public String processType;
   /** 物料价格 */
   public float materialCost;
   /** 备注 */
   public String remark;
   /** 是否手动 */
   public String manual;
   /** 计算表达式*/
   public String formulaExpress;
   /** 计算公式值*/
   public String formulaVal;
   /** 材料josn数据 */
   public String materialJson;
   /** 加工json数据*/
   public String processJson;
   /** 其他josn数据*/
   public String otherJson;
   /** sku*/
   private String sku;
   
   @Id
   public String getId() {
      return id;
   }
   
   public void setId(String newId) {
      id = newId;
   }
   
   public String getListID() {
      return listID;
   }
   
   public void setListID(String newListID) {
      listID = newListID;
   }
   
   public String getMaterialNo() {
      return materialNo;
   }
   
   public void setMaterialNo(String newMaterialNo) {
      materialNo = newMaterialNo;
   }
   
   public String getMaterialName() {
      return materialName;
   }
   
   public void setMaterialName(String newMaterialName) {
      materialName = newMaterialName;
   }
   
   public String getMaterialDesc() {
      return materialDesc;
   }
   
   public void setMaterialDesc(String newMaterialDesc) {
      materialDesc = newMaterialDesc;
   }
   
   public String getProcessType() {
      return processType;
   }
   
   public void setProcessType(String newProcessType) {
      processType = newProcessType;
   }
   
   public float getMaterialCost() {
      return materialCost;
   }
   
   public void setMaterialCost(float newMaterialCost) {
      materialCost = newMaterialCost;
   }
   
   public String getRemark() {
      return remark;
   }
   
   public void setRemark(String newRemark) {
      remark = newRemark;
   }
   
   public String getManual() {
      return manual;
   }
   
   public void setManual(String newManual) {
      manual = newManual;
   }
   
   public String getFormulaExpress() {
      return formulaExpress;
   }
   
   public void setFormulaExpress(String newFormulaExpress) {
      formulaExpress = newFormulaExpress;
   }
   
   public String getFormulaVal() {
      return formulaVal;
   }
   
   public void setFormulaVal(String newFormulaVal) {
      formulaVal = newFormulaVal;
   }
   
   public String getMaterialJson() {
      return materialJson;
   }
   
   public void setMaterialJson(String newMaterialJson) {
      materialJson = newMaterialJson;
   }
   
   public String getProcessJson() {
      return processJson;
   }
   
   public void setProcessJson(String newProcessJson) {
      processJson = newProcessJson;
   }
   
   public String getOtherJson() {
      return otherJson;
   }
   
   public void setOtherJson(String newOtherJson) {
      otherJson = newOtherJson;
   }

public String getSku() {
	return sku;
}

public void setSku(String sku) {
	this.sku = sku;
}

}