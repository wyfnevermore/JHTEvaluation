<%@page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<%String path=request.getContextPath(); %>
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<title></title>
	<link rel="stylesheet" href="<%=path%>/css/app.v2.css" type="text/css" />
	<script type="text/javascript" src="<%=path%>/Script/app.v2.js"></script> 
	<script type="text/javascript" src="<%=path%>/Script/jquery.min.js"></script>
	<script type="text/javascript" src="<%=path%>/Script/bootstrap.min.js"></script>
	<script type="text/javascript" src="<%=path%>/js/evaluation/dealing/simple/cartonCostSimple.js"></script>
	<script type="text/javascript" src="<%=path%>/Script/parsley/parsley.min.js"></script>

	<script type="text/javascript">
		var evalDetailID = window.parent.evalDetailID;
	</script>
</head>
<body>
<section class="panel panel-default">
<div class="tab-pane" style="height:auto;min-height:500px;">
         <table class="table table-striped b-t b-light text-sm"  id="cartonTable">
          <thead>
            <tr>
              <th >成品长(CM)</th>
              <th >成品宽(CM)</th>
              <th >成品高(CM)</th>	
              <th >纸箱长(CM)</th>
              <th >纸箱宽(CM)</th>
              <th >纸箱高(CM)</th>
              <th >入箱件数</th>
              <th >单位体积（立方）</th>
	      <th >纸箱单价（￥）</th>
              <th >单位纸箱成本(￥)</th>
              <th >单位衬板成本(￥)</th>
              <th >报价成本(￥)</th>
              <th style="width:80px;">操作</th>
            </tr>
          </thead>
          <tbody id="cartonData">
            <tr>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td style="display:none;"></td>
              <td style="display:none;"></td>
              <td><a onClick="getCarton(this)" href="javascript:void(0)" ><i class="fa fa-pencil"></i></a></td>
            </tr>
          </tbody>
        </table>
	     </div>
	     <div style="float: right">
            <a href="javascript:saveCartonCost();" class="btn btn-s-md btn-primary" >进入运杂费测算</a>
          </div>
	     </section>
	     
	  <!--纸箱成本测算中的编辑弹窗  -->
	<div class="modal fade" id="updateCartonCost">
		<div class="modal-dialog" style="width: 800px">
			<div class="modal-content">
				<div class="modal-body">
					<div class="row">
					<form id="cartonForm" class="form-horizontal" data-validate="parsley" method="post" onsubmit="return false" >
					<input type="hidden"  name="CartonID"  />
							<table style="text-align: right;" class="table table-striped b-t b-light text-sm">
								<tbody>
									<tr>
										<td ><label >成品长(CM)：</label ></td>
										<td><input type="text" class="form-control"  data-required="true" data-type="number" id="specLength"></td>
										<td><a onClick="selFormulaEle(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-building-o"></i></a></td>
										<td ><label >成品宽(CM)：</label ></td>
										<td><input type="text" class="form-control" data-type="number" id="specWidth"></td>
										<td><a onClick="selFormulaEle(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-building-o"></i></a></td>
									</tr>
									<tr>
										<td ><label >成品高(CM)：</label ></td>
										<td><input type="text" class="form-control" data-type="number" id="specHeight"></td>
										<td><a onClick="selFormulaEle(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-building-o"></i></a></td>
										<td><label >纸箱长度(CM)：</label></td>
										<td><input type="text" class="form-control" name="cartonLength" data-required="true" data-type="number" id="cartonLength"></td>
										<td><a onClick="selFormulaEle(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-building-o"></i></a></td>
									</tr>
									<tr>
										<td><label >纸箱宽度(CM)：</label></td>
										<td><input type="text" class="form-control" name="cartonWidth" id="cartonWidth" data-type="number" data-required="true"></td>
										<td><a onClick="selFormulaEle(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-building-o"></i></a></td>
										<td><label >纸箱高度(CM)：</label></td>
										<td><input type="text" class="form-control" name="cartonHeight" id="cartonHeight" data-type="number" data-required="true"></td>
										<td><a onClick="selFormulaEle(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-building-o"></i></a></td>
									</tr>
									<tr>
										<td><label >入箱件数：</label></td>
										<td><input type="text" class="form-control" name="qtInCarton" id="qtInCarton" data-type="number" data-required="true"></td>
										<td><a onClick="selFormulaEle(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-building-o"></i></a></td>
										<td><label >单位体积(立方)：</label></td>
										<td><input type="text" class="form-control" name="unitVolume" id="unitVolume" data-type="number" data-required="true"></td>
										<td><a onClick="selFormulaEle(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-building-o"></i></a></td>
									</tr>
									<tr>
									<td><label >纸箱单价(￥)：</label></td>
										<td><input type="text" class="form-control" name="unitCartonCost" id="unitCartonCost" data-required="true" data-type="number"></td>
										<td><a onClick="selFormulaEle(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-building-o"></i></a></td>
										<td></td>
										<td></td>
										<td></td>
									</tr>
									<tr>
										<td><label >单位纸箱成本(￥)：</label></td>
										<td><input type="text" class="form-control" name="unitCartCost" id="unitCartCost" data-type="number" data-required="true"></td>
										<td><a onClick="calFormulaResult(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-refresh"></i></a></td>
										<td><label >单位衬板成本(￥)：</label></td>
										<td><input type="text" class="form-control" name="unitPanelCost" id="unitPanelCost" data-type="number" data-required="true"></td>
										<td><a onClick="calFormulaResult(this)" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-refresh"></i></a></td>
									</tr>
									<tr>
										<td><label >运算符号：</label></td>
										<td><a style="margin-right:30px;" onClick="formulaCalAdd()" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-plus"></i></a>
											<a style="margin-right:30px;" onClick="formulaCalReduce()" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-minus"></i></a>
											<a style="margin-right:30px;" onClick="formulaCalMultiply()" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-times"></i></a>
											<a style="margin-right:30px;" onClick="formulaCalDivide()" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-italic"></i></a>
											<a style="margin-right:30px;" onClick="formulaCalrBacketL()" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-chevron-left"></i></a>
											<a style="margin-right:30px;" onClick="formulaCalrBacketR()" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-chevron-right"></i></a>
										</td>
										<td></td>
										<td></td>
										<td></td>
										<td></td>
									</tr>
									<tr>
										<td><label>计算表达式：</label></td>
										<td colspan="4"><input id="formulaInput1" name="calculExpression" type="text" class="form-control"></td>
										<td><a onClick="clearFormulaInput()" href="javascript:void(0)" data-toggle="modal"><i class="fa fa-wrench"></i></a></td>
									</tr>
									<tr>
										<td><label>计算公式：</label></td>
										<td colspan="4"><input id="formulaInput2" name="calculFormula" type="text" class="form-control"></td>
										<td></td>
									</tr>
									<tr>
									<td></td>
									<td></td>
									<td></td>
									<td><label >报价成本(￥)：</label></td>
										<td><input type="text" class="form-control" name="quoteCost" id="quoteCost" data-required="true" data-type="number" ></td>
										<td></td>
									</tr>
									
								</tbody>
							</table>
						<div>
						<a style="width:80px;height:30px;float:right;margin-right:35px"  href="javascript:updateCarton();" data-taggle="modal" class="btn btn-default btn-sm">确定</a>
				     	</div>
				     	</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  